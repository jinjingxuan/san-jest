{
  "name": "san-jest-test",
  "version": "0.1.0",
  "description": "jest .san file support",
  "scripts": {
    "pretest": "jest --clearCache",
    "test": "jest",
    "posttest": "rm -rf ./cache/"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "san"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.san$": "san-jest"
    },
    "globals": {
      "san-jest": {
        "rollup-config-path": "custom.config.js"
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "dependencies": {
    "axios": "^0.22.0",
    "san-jest": "^0.1.3",
    "ts-jest": "^27.0.5",
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.6",
    "@babel/runtime": "^7.15.4",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "babel-jest": "^27.2.4",
    "debug": "^4.3.2",
    "fs-extra": "^10.0.0",
    "hash-sum": "^2.0.0",
    "jest": "^27.2.4",
    "less": "^4.1.2",
    "postcss": "^8.3.9",
    "rollup": "^2.58.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-san": "^1.0.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "san": "^3.10.10",
    "san-test-utils": "^0.1.3",
    "typescript": "^4.4.3"
  },
  "keywords": [
    "san",
    "jest"
  ],
  "author": "wangjinghao@baidu.com",
  "license": "ISC"
}
