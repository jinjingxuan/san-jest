{
  "name": "san-jest-test",
  "version": "0.1.0",
  "description": "jest .san file support",
  "scripts": {
    "pretest": "jest --clearCache",
    "test": "jest",
    "posttest": "rm -rf ./cache/"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "san"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.san$": "san-jest"
    },
    "globals": {
      "san-jest": {
        "rollup-config-path": "custom.config.js"
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/runtime": "^7.15.4",
    "axios": "^0.22.0",
    "babel-jest": "^27.2.4",
    "jest": "^27.2.4",
    "san": "^3.10.10",
    "san-jest": "^0.1.0",
    "rollup-plugin-san": "^1.0.2",
    "san-test-utils": "^0.1.3",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "keywords": [
    "san",
    "jest"
  ],
  "author": "wangjinghao@baidu.com",
  "license": "ISC"
}
