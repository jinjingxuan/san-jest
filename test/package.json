{
  "name": "san-jest-test",
  "version": "1.0.0",
  "description": "jest .san file support test cases",
  "scripts": {
    "test": "jest --no-cache"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "ts",
      "san"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.san$": "san-jest"
    },
    "globals": {
      "san-jest": {
        "templateCompileOptions": {},
        "styleCompileOptions": {},
        "scriptCompileOptions": {}
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.6",
    "@babel/runtime": "^7.15.4",
    "axios": "^0.24.0",
    "babel-jest": "^27.2.4",
    "jest": "^27.2.4",
    "less": "^4.1.2",
    "postcss": "^8.3.9",
    "san": "^3.10.10",
    "san-jest": "link:../",
    "san-test-utils": "^0.1.3",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "keywords": [
    "san",
    "jest"
  ],
  "author": "wangjinghao@baidu.com",
  "license": "MIT"
}
